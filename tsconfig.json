{
  "compilerOptions": {
    "lib": ["es2020", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "incremental": true,
    "target": "es2020",
    "sourceMap": true,
    "declaration": true,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  }
}